{
  "name": "isomorphic-demo",
  "description": "Demo of an isomorphic app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "webpack --config ./webpack.config-server.js/",
    "build:client": "webpack --config ./webpack.config-client.js/",
    "build:assets": "node scripts/copy-assets.js",
    "build": "npm run build:client && npm run build:server && npm run build:assets",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node ./build/server.js",
    "start:dev": "NODE_ENV=development npm run build && NODE_ENV=development node ./build/server.js",
    "start:dev:client": "webpack-dev-server --config ./webpack.config-client.js/"
  },
